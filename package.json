{
  "name": "zkevm-circuits",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test:circuits": "mocha test/stark/**/*.test.js",
    "test:circuits:bn128": "mocha test/stark/bn128/**/*.test.js",
    "test:circuits:bn128custom": "mocha test/stark/bn128.custom/**/*.test.js",
    "test:circuits:gl": "mocha test/stark/gl/**/*.test.js",
    "test:recursive2:batch": "node test/recursive2/main_genrecursive2_test.js -p src/publics/batch_publics.json -t tmp/test_recursive2_batch_publics.js && mocha tmp/test_recursive2_batch_publics.js",
    "test:recursive2:batch:eip4844": "node test/recursive2/main_genrecursive2_test.js -p src/publics/batch_eip4844_publics.json -t tmp/test_recursive2_batch_eip4844_publics.js && mocha tmp/test_recursive2_batch_eip4844_publics.js",
    "test:recursive2:blob": "node test/recursive2/main_genrecursive2_test.js -p src/publics/blob_outer_publics.json -t tmp/test_recursive2_blob_outer_publics.js && mocha tmp/test_recursive2_blob_outer_publics.js",
    "test:final:batch": "node test/final/main_genfinal_test.js -p src/publics/batch_publics.json -t tmp/test_final_batch_publics.js && mocha tmp/test_final_batch_publics.js",
    "test:final:blob": "node test/final/main_genfinal_test.js -p src/publics/blob_outer_publics.json -t tmp/test_final_blob_outer_publics.js && mocha tmp/test_final_blob_outer_publics.js",
    "test:blob_outer": "mocha test/circuits/blob_outer.circuit.test.js",
    "test:recursive1:circom": "npm run compressor_verifier_gencircom && npm run recursive1_gencircom && npm run recursive1_compile",
    "test:recursive2:circom": "npm run recursive1_verifier_gencircom && npm run recursive2_gencircom && npm run recursive2_compile",
    "compressor_verifier_gencircom": "NODE_OPTIONS=--max-old-space-size=32000 node src/pil2circom/main_pil2circom.js --skipMain -p test/pil2circom/compressor/compressor.pil -s test/pil2circom/compressor/compressor.starkinfo.json -i test/pil2circom/compressor/compressor.verifierinfo.json -v test/pil2circom/compressor/compressor.verkey.json -o tmp/compressor.verifier.circom",
    "recursive1_gencircom": "node src/main_gencircom.js -s test/pil2circom/compressor/compressor.starkinfo.json -p src/publics/batch_publics.json -n compressor.verifier.circom -t src/recursion/templates/recursive1.circom.ejs -o tmp/recursive1.circom",
    "recursive1_compile": "circom -l src/pil2circom/circuits.gl --O1 --prime goldilocks --r1cs --wasm --verbose tmp/recursive1.circom -o tmp/",
    "recursive1_verifier_gencircom": "NODE_OPTIONS=--max-old-space-size=32000 node src/pil2circom/main_pil2circom.js --skipMain --verkeyInput -p test/pil2circom/recursive1/recursive1.pil -s test/pil2circom/recursive1/recursive1.starkinfo.json -i test/pil2circom/recursive1/recursive1.verifierinfo.json -v test/pil2circom/recursive1/recursive1.verkey.json -o tmp/recursive1.verifier.circom",
    "recursive2_gencircom": "node src/main_gencircom.js -s test/pil2circom/recursive1/recursive1.starkinfo.json -p src/publics/batch_publics.json -n recursive1.verifier.circom -t src/recursion/templates/recursive2.circom.ejs -b test/pil2circom/recursive1/recursive1.verkey.json -o tmp/recursive2.circom",
    "recursive2_compile": "circom -l src/pil2circom/circuits.gl --O1 --prime goldilocks --r1cs --wasm --verbose tmp/recursive2.circom -o tmp/",
    "plonktest_compile": "circom --O1 --prime goldilocks --r1cs --sym --wasm --verbose test/plonk2pil/plonktest.gl.circom -o tmp",
    "plonktest_wc": "snarkjs wc tmp/plonktest.gl_js/plonktest.gl.wasm test/plonk2pil/plonkinput.json tmp/plonktest.gl.wtns",
    "plonktest_C12_setup": "NODE_OPTIONS=--max-old-space-size=32000 node test/plonk2pil/main_plonksetup.js -r tmp/plonktest.gl.r1cs -p tmp/plonktest.gl.pil -c tmp/plonktest.gl.const -e tmp/plonktest.gl.exec --cols=12",
    "plonktest_C12_exec": "NODE_OPTIONS=--max-old-space-size=32000 node test/plonk2pil/main_plonkexec.js -w tmp/plonktest.gl_js/plonktest.gl.wasm -i test/plonk2pil/plonkinput.json -p tmp/plonktest.gl.pil -e tmp/plonktest.gl.exec -m tmp/plonktest.gl.commit",
    "plonktest_C12_verify": "NODE_OPTIONS=--max-old-space-size=32000 node node_modules/pilcom/src/main_pilverifier.js tmp/plonktest.gl.commit -c tmp/plonktest.gl.const -p tmp/plonktest.gl.pil"
  },
  "dependencies": {
    "@0xpolygonhermez/zkevm-commonjs": "github:0xpolygonhermez/zkevm-commonjs#v6.0.0-fork.9",
    "circom_runtime": "^0.1.25",
    "circomlib": "^2.0.5",
    "circomlibjs": "^0.1.7",
    "ethers": "^6.8.1",
    "json-bigint": "^1.0.0",
    "pil2-compiler": "https://github.com/0xPolygonHermez/pil2-compiler.git#develop",
    "pilcom": "^0.0.24",
    "protobufjs": "^7.3.2",
    "r1csfile": "^0.0.47"
  },
  "devDependencies": {
    "@iden3/bigarray": "^0.0.2",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "circom_tester": "^0.0.20",
    "logplease": "^1.2.15",
    "mocha": "^10.0.0",
    "temporary": "^1.1.0",
    "tmp-promise": "^3.0.3"
  },
  "author": "",
  "license": "(MIT OR Apache-2.0)",
}
